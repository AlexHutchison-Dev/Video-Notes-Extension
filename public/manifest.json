{
  "manifest_version": 2,
  "name": "Video-Notes",
  "description": "Time stamped video lecturenotes",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.udemy.com/course/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "index.html",
    "default_title": "Video-Notes"
  }
  
}
